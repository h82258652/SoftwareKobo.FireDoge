<UserControl x:Class="SoftwareKobo.FireDoge.Controls.FaviconIcon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SoftwareKobo.FireDoge.Controls"
             xmlns:model="clr-namespace:SoftwareKobo.FireDoge.Models"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance Type=model:TabPage}"
             Loaded="FaviconIcon_Loaded">
    <UserControl.Resources>
        <ResourceDictionary Source="/Styles/TabHeaderStyle.xaml" />
    </UserControl.Resources>
    <Grid>
        <Image x:Name="FaviconImage"
               Style="{StaticResource FaviconImageStyle}"
               Source="/Assets/Images/blank_page_ico.png" />
    </Grid>
</UserControl>